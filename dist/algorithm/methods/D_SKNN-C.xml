<algorithm_specification>
  <name>KNN Based on Dempster-Shafer Theory</name>
  <nParameters>3</nParameters>
  <seed>0</seed>
  <nOutput>1</nOutput>
  <parameter>
    <name>K Value</name>
    <type>integer</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>100</upperB>
    </domain>
    <default>3</default>
  </parameter>
  <parameter>
    <name>ALPHA</name>
    <type>real</type>
    <domain>
      <lowerB>0.01</lowerB>
      <upperB>100.0</upperB>
    </domain>
    <default>0.95</default>
  </parameter>
  <parameter>
    <name>BETA</name>
    <type>real</type>
    <domain>
      <lowerB>0.0000001</lowerB>
      <upperB>10000.0</upperB>
    </domain>
    <default>1</default>
  </parameter>
</algorithm_specification>
