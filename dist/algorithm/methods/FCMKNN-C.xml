<algorithm_specification>
  <name>Fuzzy C-Means for KNN classifier</name>
  <nParameters>4</nParameters>
  <seed>1</seed>
  <nOutput>1</nOutput>
  <parameter>
    <name>K Value</name>
    <type>integer</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>100</upperB>
    </domain>
    <default>3</default>
  </parameter>
  <parameter>
    <name>M</name>
    <type>real</type>
    <domain>
      <lowerB>0.01</lowerB>
      <upperB>100.0</upperB>
    </domain>
    <default>2.0</default>
  </parameter>
  <parameter>
    <name>Max Iterations</name>
    <type>integer</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>10000</upperB>
    </domain>
    <default>50</default>
  </parameter>
  <parameter>
    <name>Delta</name>
    <type>real</type>
    <domain>
      <lowerB>0.0000001</lowerB>
      <upperB>10000.0</upperB>
    </domain>
    <default>0.01</default>
  </parameter>
</algorithm_specification>
