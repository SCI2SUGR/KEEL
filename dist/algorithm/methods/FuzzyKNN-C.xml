<algorithm_specification>
  <name>Fuzzy KNN Classifier</name>
  <nParameters>4</nParameters>
  <seed>0</seed>
  <nOutput>1</nOutput>
  <parameter>
    <name>K Value</name>
    <type>integer</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>100</upperB>
    </domain>
    <default>3</default>
  </parameter>
  <parameter>
    <name>M</name>
    <type>real</type>
    <domain>
      <lowerB>0.01</lowerB>
      <upperB>100.0</upperB>
    </domain>
    <default>2.0</default>
  </parameter>
  <parameter>
    <name>Initialization</name>
    <type>list</type>
    <domain>
      <item>CRISP</item>
      <item>CLASS-MEAN</item>
	  <item>KNN</item>
    </domain>
    <default>CRISP</default>   
  </parameter>
  <parameter>
    <name>k Value (init)</name>
    <type>integer</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>100</upperB>
    </domain>
    <default>3</default>
  </parameter>
</algorithm_specification>
