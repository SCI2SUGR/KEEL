<algorithm_specification>
  <name>GA for Fuzzy KNN Classifier</name>
  <nParameters>5</nParameters>
  <seed>1</seed>
  <nOutput>1</nOutput>
  <parameter>
    <name>K value</name>
    <type>integer</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>1000</upperB>
    </domain>
    <default>3</default>
  </parameter>
  <parameter>
    <name>Population Size</name>
    <type>integer</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>1000</upperB>
    </domain>
    <default>50</default>
  </parameter>
  <parameter>
    <name>Maximum generations</name>
    <type>integer</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>1000</upperB>
    </domain>
    <default>10</default>
  </parameter>
  <parameter>
    <name>Crossover probability</name>
    <type>real</type>
    <domain>
      <lowerB>0.0001</lowerB>
      <upperB>1.0</upperB>
    </domain>
    <default>0.8</default>
  </parameter>
  <parameter>
    <name>Mutation probability</name>
    <type>real</type>
    <domain>
      <lowerB>0.0001</lowerB>
      <upperB>1.0</upperB>
    </domain>
    <default>0.01</default>
  </parameter>
</algorithm_specification>
