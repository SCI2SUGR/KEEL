  <parameter>
    <name>Connectivity</name>
    <type>real</type>
    <default>0.5</default>
  </parameter>
  <parameter>
    <name>P_bp</name>
    <type>real</type>
    <default>0.25</default>
  </parameter>
  <parameter>
    <name>P_param</name>
    <type>real</type>
    <default>0.1</default>
  </parameter>
  <parameter>
    <name>P_struct</name>
    <type>real</type>
