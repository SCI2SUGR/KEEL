<algorithm_specification>
  <name>GA with Neural Network</name>
  <nParameters>23</nParameters>
  <seed>1</seed>
  <nOutput>1</nOutput>
  <parameter>
    <name>Hidden_layers</name>
    <type>integer</type>
    <default>2</default>
  </parameter>
  <parameter>
    <name>Hidden_nodes</name>
    <type>integer</type>
    <default>15</default>
  </parameter>
  <parameter>
    <name>Transfer</name>
    <type>list</type>
    <domain>
      <item>Log</item>
      <item>Htan</item>
      <item>Lin</item>
    </domain>
    <default>Htan</default>
  </parameter>
  <parameter>
    <name>Eta</name>
    <type>real</type>
    <default>0.15</default>
  </parameter>
  <parameter>
    <name>Alpha</name>
    <type>real</type>
    <default>0.10</default>
  </parameter>
  <parameter>
    <name>Lambda</name>
    <type>real</type>
    <default>0.0</default>
  </parameter>
  <parameter>
    <name>Test_data</name>
    <type>list</type>
    <domain>
      <item>true</item>
      <item>false</item>
    </domain>
    <default>true</default>
  </parameter>
  <parameter>
    <name>Validation_data</name>
    <type>list</type>
    <domain>
      <item>true</item>
      <item>false</item>
    </domain>
    <default>false</default>
  </parameter>
  <parameter>
    <name>Cross_validation</name>
    <type>list</type>
    <domain>
      <item>true</item>
      <item>false</item>
    </domain>
    <default>false</default>
  </parameter>
  <parameter>
    <name>BP_cycles</name>
    <type>integer</type>
    <default>10000</default>
  </parameter>
  <parameter>
    <name>Improve</name>
    <type>real</type>
    <default>0.01</default>
  </parameter>
  <parameter>
    <name>Problem</name>
    <type>list</type>
    <domain>
      <item>Regression</item>
    </domain>
    <default>Regression</default>
    <disabled/>
  </parameter>
  <parameter>
    <name>Tipify_inputs</name>
    <type>list</type>
    <domain>
      <item>true</item>
      <item>false</item>
    </domain>
    <default>true</default>
  </parameter>
  <parameter>
    <name>Verbose</name>
    <type>list</type>
    <domain>
      <item>true</item>
      <item>false</item>
    </domain>
    <default>true</default>
  </parameter>
  <parameter>
    <name>SaveAll</name>
